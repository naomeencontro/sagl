<dtml-comment>
connection_id : dbcon_openlegis
arguments :  num_protocolo ano_protocolo dat_protocolo hor_protocolo tip_protocolo tip_processo=0 txt_interessado tip_documento txt_assunto_ementa num_paginas txt_observacao ind_anulado=0
max_rows : 1
max_cache : 100
</dtml-comment>

INSERT INTO protocolo 
(num_protocolo,
ano_protocolo,
dat_protocolo,
hor_protocolo,
tip_protocolo,
tip_processo,
txt_interessado,
tip_documento,
txt_assunto_ementa,
num_paginas,
txt_observacao,
ind_anulado)
 
VALUES

  ( <dtml-sqlvar      num_protocolo         type="int" optional>,
    YEAR(NOW()),
    NOW(),
    NOW(),
    <dtml-sqlvar      tip_protocolo         type="int">,
    <dtml-sqlvar      tip_processo          type="int">,
    <dtml-sqlvar      txt_interessado       type="string">,
    <dtml-sqlvar      tip_documento         type="int">,
    <dtml-sqlvar      txt_assunto_ementa    type="string">,
    <dtml-sqlvar      num_paginas           type="int" optional>,
    <dtml-sqlvar      txt_observacao        type="string" optional>,
    <dtml-sqlvar      ind_anulado           type="int">);

