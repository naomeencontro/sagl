<!DOCTYPE html>
<html class="no-js" lang="pt-br">
<dtml-if expr="_.has_key('QUERY_STRING') and QUERY_STRING=='retry=1' and portal_membership.isAnonymousUser()">
  <dtml-call expr="REQUEST['RESPONSE'].expireCookie('__ac', path='/')">
  <dtml-call expr="REQUEST['RESPONSE'].redirect(portal_url() +'/index_html?retry=0')">
</dtml-if>
<dtml-call "RESPONSE.setHeader('Content-Type','text/html; charset=utf-8')">
<dtml-call "RESPONSE.setHeader('Expires', 'Sat, 1 Jan 2000 00:00:00 GMT')">
<dtml-call "RESPONSE.setHeader('Cache-Control', 'private, no-cache, no-store, must-revalidate, post-check=0, pre-check=0')">
<head>
  <meta charset="utf-8">
  <meta name="HandheldFriendly" content="True">
  <meta name="MobileOptimized" content="320">
  <meta name="author" content="OpenLegis">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.6, minimum-scale=0.4">
  <link rel="shortcut icon" type="image/x-icon" href="&dtml-portal_url;/imagens/favicon.ico">
  <dtml-var standard_css_slot>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/modernizr.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/jquery.min.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/acessibilidade.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/jquery.loader.min.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/jquery-ui.min.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/jquery.jui_dropdown.min.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/jquery.plugin.min.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/jquery.datepick.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/jquery.datepick-pt-BR.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/jquery.fancybox.pack.js"></script>
  <script type='text/javascript' src='&dtml-portal_url;/javascript/geral.js'></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/data_validar_jvsc"></script>
  <link href="&dtml-portal_url;/generico/dhtmlxcombo.css" media="all" rel="stylesheet" type="text/css" />
  <script type="text/javascript" src="&dtml-portal_url;/javascript/dhtmlxcommon.js"></script>
  <script type="text/javascript" src="&dtml-portal_url;/javascript/dhtmlxcombo.js"></script>

  <dtml-if expr="_.has_key('QUERY_STRING') and QUERY_STRING == 'retry=0'">
    <script type="text/javascript">
      <!--
        alert("Nome de usuário e/ou senha não conferem!");
      //-->
    </script>
  </dtml-if>
  <script type="text/javascript">
    $(document).ready(function() {
      document.title = $(".firstHeading").text() + ' — ' + document.title;
    });
  </script>

  <title>
    OpenLegis - Processo Legislativo Eletrônico
  </title>

<script type="text/javascript">

(function ($) {
  $(function () {
    //$('input[type=submit]').hide();
    document.onkeypress = function (event) {
      var form = pesq_avancada_form;
      form.action = "protocolo_pesquisar_proc";
      if (event.keyCode === 13) {
        form.submit();
      }
    };
  });
}(jQuery));

$(function() {
        $('#dt_apres').datepick();
        $('#dt_apres2').datepick();
});
</script>

<script>
var lista_autores;
function doOnLoad() {
	lista_autores = new dhtmlXCombo("combo_zone", "hdn_cod_autor", 240);
	lista_autores.load("&dtml-portal_url;/pysc/autores_pesquisar_pysc");
	lista_autores.enableFilteringMode(true);
}
function enableComplete(mode) {
	lista_autores.enableAutocomplete(mode);
}

  function interessado() {
    if (document.pesq_avancada_form.txa_txt_interessado.value != '') {
      document.pesq_avancada_form.rad_tip_processo[0].checked = true;
      document.pesq_avancada_form.txt_autor.value = '';
    }

  }

</script>
</head>
<body class="fadeIn" onload="doOnLoad();">

<div id="preloader"></div>

<div id="visual-portal-wrapper-container">
  <dtml-if expr="_.has_key('impressao')">
    <dtml-var printable_html_top_bar>
  <dtml-elif expr="_.has_key('iframe') or REQUEST.SESSION.get('iframe')">
    <dtml-call expr="REQUEST.SESSION.set('iframe', 1)">
  <dtml-else>
      <dtml-var standard_html_top_bar>
      <div id="corpo">
       <div class="linha">
       <dtml-var standard_html_lateral>
       <div id="conteudo">
  </dtml-if>
<dtml-if expr="REQUEST.has_key('portal_status_message')">
  <p id="system-message"><dtml-var portal_status_message></p>
</dtml-if>


<h1 class="firstHeading">Protocolo Geral</h1> 

<dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Protocolo'])">
    <ul>
        <li>
            <a href="&dtml-portal_url;/cadastros/protocolo/protocolo_administrativo_form">Protocolar Documento</a>
        </li>
        <li>
            <a href="&dtml-portal_url;/cadastros/protocolo/protocolo_legislativo_form">Protocolar Matéria</a>
        </li>
        <li>
            <a href="&dtml-portal_url;/cadastros/protocolo/protocolo_anular_form">Anular Protocolo</a>
        </li>
    </ul>
</dtml-if>

<form class="pf-form" name="pesq_avancada_form" id="pesq_avancada_form" action="protocolo_pesquisar_proc">
  <input type="hidden" name="incluir" value="<dtml-var incluir missing>" />
  <input type="hidden" name="existe_ocorrencia" value="0" />
  <fieldset>
  <div id="inputs">
     <div class="pf-element pf-full-width">
        <label><span class="pf-label">Tipo de Protocolo</span></label>
        <div class="pf-group">
          <dtml-in expr="[('Recebido', '0'), ('Enviado', '1'), ('Ambos', '')]">     
           <input type="radio" name="rad_tip_protocolo" value="<dtml-var expr="_['sequence-item']">"
                  id="rad_tip_protocolo<dtml-var expr="_['sequence-item']">"
                <dtml-if expr="_['sequence-item']=='0'">checked="1"</dtml-if> />
             <label style="vertical-align: top;" for="rad_tip_protocolo<dtml-var expr="_['sequence-item']">"><dtml-var expr="_['sequence-key']"> &nbsp;</label>
          </dtml-in>
        </div>
     </div>

     <div class="pf-element pf-full-width">
        <label for="txt_num_protocolo"><span class="pf-label">Número</span></label>
        <div class="pf-group">
          <input class="pf-field" id="txt_num_protocolo" type="text" name="txt_num_protocolo" null="" missing="" style="min-width: 94px; max-width: 94px; width: 20%;" maxlength="6" />
          <label for="txt_ano_protocolo">&nbsp;Ano
            <input class="pf-field" id="txt_ano_protocolo" type="text" name="txt_ano_protocolo" null="" style="min-width: 86px; max-width: 86px; width: 20%;" maxlength="4" />
          </label>
        </div>
     </div> 

     <div class="pf-element pf-full-width">
       <label><span class="pf-label">Período de Protocolo:</span></label>
        <div class="pf-group">
          <label for="dt_apres">
            <input class="pf-field" type="text" id="dt_apres" name="dt_apres" style="min-width: 94px; max-width: 94px; width: 20%;" maxlength=10 onblur="formata_data(form.dt_apres)" />
          </label>
          <label for="dt_apres2">&nbsp;a&nbsp;
            <input class="pf-field" type="text" id="dt_apres2" name="dt_apres2" style="min-width: 102px; max-width: 102px; width: 20%;" maxlength="10" onblur="formata_data(form.dt_apres2)" />
          </label>
        </div>
     </div> 

     <div class="pf-element pf-full-width">
        <label><span class="pf-label">Natureza do Processo</span></label>
        <div class="pf-group">
             <input name="rad_tip_processo" value="0" id="rad_tip_processo0" type="radio" >
             <label for="rad_tip_processo0">Administrativo</label>
             <input name="rad_tip_processo" value="1" id="rad_tip_processo1" type="radio" >
             <label for="rad_tip_processo1">Legislativo</label>
             <input name="rad_tip_processo" value="" id="rad_tip_processoN" checked="1" type="radio" >
             <label for="rad_tip_processo">Ambos</label>
        </div>
     </div>

     <div class="pf-element pf-full-width">
        <label for="lst_tip_documento"><span class="pf-label">Tipo de Documento</span></label>
          <select class="pf-field" id="lst_tip_documento" name="lst_tip_documento" style="max-width: 240px; width: 100%;">
            <option value="">Selecione</option>
            <dtml-in expr="zsql.tipo_documento_administrativo_obter_zsql(ind_excluido=0)">
              <option value="<dtml-var tip_documento missing>">
                <dtml-var sgl_tipo_documento> - <dtml-var des_tipo_documento>
              </option>
            </dtml-in>
          </select>
     </div>

     <div class="pf-element pf-full-width">
        <label for="txa_txt_interessado"><span class="pf-label">Interessado</span></label></th>
          <input class="pf-field" id="txa_txt_interessado" type="text" name="txa_txt_interessado" null="" style="max-width: 230px; width: 100%;" onBlur="interessado()" />
     </div>

     <div class="pf-element pf-full-width">
        <label for="lst_tip_materia"><span class="pf-label">Tipo de Matéria</span></label>
          <select class="pf-field" id="lst_tip_materia" name="lst_tip_materia" style="max-width: 240px; width: 100%;">
            <option value="">Selecione</option>
            <dtml-in expr="zsql.tipo_materia_legislativa_obter_zsql(ind_excluido=0)">
              <option value="<dtml-var tip_materia missing>">
                <dtml-var sgl_tipo_materia> - <dtml-var des_tipo_materia>
              </option>
            </dtml-in>
          </select>
     </div>

     <div class="pf-element pf-full-width">
       <label for="txt_autor"><span class="pf-label">Autor da Matéria</span></label>
       <div class="pf-group">
 	<div id="combo_zone" style="width:235px;"></div>
      </div>
     </div> 

     <div class="pf-element pf-full-width">
        <label for="txt_assunto"><span class="pf-label">Assunto</span></label></th>
          <input class="pf-field" id="txt_assunto" type="text" name="txt_assunto" null="" style="max-width: 230px; width: 100%;" />
     </div>

     <div class="pf-element pf-buttons">
          <input class="pf-button btn botao" type="submit" name="btn_protocolo_pesquisar" value="Pesquisar" />
          <input class="pf-button btn botao" type="reset" name="btn_protocolo_limpar" value="Limpar" />
     </div>

   </div>
  </fieldset>
</form>
<dtml-var standard_html_footer>
