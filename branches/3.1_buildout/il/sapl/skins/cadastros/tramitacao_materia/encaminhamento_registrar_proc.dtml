<dtml-try>
   <dtml-call expr="zsql.tramitacao_registrar_encaminhamento_zsql(cod_tramitacao=int(cod_tramitacao))">
<dtml-except>
  <dtml-let mensagem="'Houve um erro no banco de dados!'">
    <dtml-var mensagem_emitir>
  </dtml-let>  
<dtml-else>
  <dtml-let mensagem="'Tramitação encaminhada com sucesso!'" sucesso="1" url="'caixa_saida_html'">
    <dtml-var mensagem_emitir>
  </dtml-let>
</dtml-try>
