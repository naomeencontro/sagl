<dtml-var standard_html_header>

<a id="ajuda" href="&dtml-portal_url;/ajuda/status_tramitacao">Ajuda</a> 

<h1 class="firstHeading"> Tabelas Auxiliares </h1> 

<table>
  <tr> 
    <td class="form-label">| <a href="status_tramitacao_form">
      Incluir Status Tramitação</a> |</td>
   </tr>
</table>

<fieldset><legend>Status Tramitação</legend>
<dtml-in expr="zsql.status_tramitacao_obter_zsql()"> 
    <dtml-if sequence-start>
       <table>
          <tr> 
            <th width="20%" class="form-label">Sigla</th>
            <th width="20%" class="form-label">Indicador</th>
            <th width="60%" class="form-label">Descrição</th>
          </tr>
    </dtml-if sequence-start>
          <tr>
            <td width="20%" class="form-element">
            <a href="status_tramitacao_mostrar_proc?cod_status=<dtml-var cod_status>">
             <dtml-var sgl_status></a></td>
           <td width="20%" class="form-text">
             <dtml-if ind_fim_tramitacao>Fim
             <dtml-elif ind_retorno_tramitacao>Retorno
             </dtml-if></td>
           <td width="60%" class="form-text"><dtml-var des_status></td>
         </tr>
    <dtml-if sequence-end>
       </table>
    </dtml-if sequence-end>
  <dtml-else>
       <table>
          <tr><td class="form-text">Nenhum Status de Tramitacao cadastrado.</td></tr>
       </table>
</dtml-in>
</fieldset>
<dtml-var standard_html_footer>
