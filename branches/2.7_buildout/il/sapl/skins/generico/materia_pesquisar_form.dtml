<!DOCTYPE html>
<html lang="pt-br">
<dtml-if expr="_.has_key('QUERY_STRING') and QUERY_STRING=='retry=1' and portal_membership.isAnonymousUser()">
  <dtml-call expr="REQUEST['RESPONSE'].expireCookie('__ac', path='/')">
