  background: -moz-linear-gradient(top, #aaaaaa 0%, #4a4a4e);
}

.navContainer {
  width: 100%;
  margin: 0 auto;
  background-image: linear-gradient(top, rgb(255,255,255) 31%, rgb(244,244,244) 100%);
  background-image: -o-linear-gradient(top, rgb(255,255,255) 31%, rgb(244,244,244) 100%);
  background-image: -moz-linear-gradient(top, rgb(255,255,255) 31%, rgb(244,244,244) 100%);
  background-image: -webkit-linear-gradient(top, rgb(255,255,255) 31%, rgb(244,244,244) 100%);
  background-image: -ms-linear-gradient(top, rgb(255,255,255) 31%, rgb(244,244,244) 100%);
  border-bottom: 3px solid #e4e4e4;
}

#funcoes {
  line-height: 15px;
  font-size: 14px;
  text-align: left;
  margin: 0 auto;
  max-width: 1140px;
}

#funcoes > ul {
  width: auto;
  list-style-type: none;
  padding: 0;
  margin: 0;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  -o-border-radius: 2px;
  border-radius: 2px;
}

#funcoes > ul li#responsive-tab {
  display: none;
}

#funcoes > ul li {
  display: inline-block;
  *display: inline;
  zoom: 1;
}

#funcoes > ul li.right {
  float: right;
}

#funcoes > ul li.has-sub {
  position: relative;
  margin-right: -3px;
}

#funcoes > ul li.has-sub:hover ul {
  display: block;
}

#funcoes > ul li.has-sub ul {
  display: none;
  min-width: 200px;
  list-style-type: none;
  border-top: 0 none;
  z-index: 1000;
  position: absolute;
  padding: 5px 0;
  font-size: 13px;
  background-color: #fff;
  -webkit-background-clip: padding-box;
  background-clip: padding-box;
  border: 1px solid #ccc;
  border: 1px solid rgba(0,0,0,.15);
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
  -webkit-box-shadow: 0 6px 12px rgba(0,0,0,.175);
  box-shadow: 0 6px 12px rgba(0,0,0,.175);

}

#funcoes > ul li.has-sub ul li {
  display: block;
  background: #fff;
}

#funcoes > ul li.has-sub > a {
  background-image: url(&dtml-portal_url;/imagens/caret.png);
  background-repeat: no-repeat;
  background-position: 95% -73%;
}

#funcoes > ul li.has-sub > a.active,
#funcoes > ul li.has-sub > a:hover {
  background-image: url(&dtml-portal_url;/imagens/caret.png);
  background-repeat: no-repeat;
  background-position: 95% -73%;
}

#funcoes > ul li a {
  display: block;
  padding: 10px 20px 10px 15px;
  text-decoration: none;
  color: #555;
  -webkit-transition: 0.2s ease-in-out;
  -moz-transition: 0.2s ease-in-out; 
  -o-transition: 0.2s ease-in-out
}

#funcoes > ul li a.active,
#funcoes > ul li a:hover {
  background: #e7e7e7;
  color: #333;
}

#funcoes li.has-sub:hover > a {
   background-color: #e7e7e7;
   color:#333;
}

@media (max-width: 650px) {

.navContainer {
 border-bottom: none;
}
  #funcoes > ul {
    width: 100%;
  }
  #funcoes > ul li#responsive-tab {
    display: block;
  }
  #funcoes > ul li#responsive-tab a {
    font-size: 15px;
    font-weight: bold;
    text-transform: uppercase;
    color: #f1f1f1;
    padding: 15px 15px 15px 10px;
    background: &dtml-cor_principal; url('imagens/menu-nav.png') no-repeat;
    background-position: 94% -54%;
  }
  #funcoes > ul li#responsive-tab a:hover {
    background-color: &dtml-cor_principal;;
    color: #fff;
    background-position: 94% 150%;
  }
  #funcoes > ul li {
    display: none;
  }
  #funcoes > ul li.right {
    float: none;
  }
  #funcoes > ul li.has-sub {
    position: relative;
  }
  #funcoes > ul li.has-sub ul {
    display: block;
    position: static;
    width: 100%;
    background: #ffffff;
    border: 0 none;
    -webkit-box-shadow: none; 
    box-shadow: none; 
  }
  #funcoes > ul li.has-sub ul li {
    display: block !important;
  }
  #funcoes > ul li.has-sub ul li a span {
    display: block;
    padding-left: 24px;
  }
  #funcoes > ul li.has-sub > a {
    background-image: none;
  }
}

@media (min-width: 650px) {
  #funcoes > ul > li.collapsed {
    display: inline-block !important;
    *display: inline;
    zoom: 1;
  }
  #funcoes > ul ul li.collapsed {
    display: block !important;
  }
}

@media (min-width: 650px) {
  #funcoes > ul > li.collapsed {
    display: inline-block !important;
    *display: inline;
    zoom: 1;
  }
  #funcoes > ul ul li.collapsed {
    display: block !important;
  }
}

.cd-top.cd-is-visible {
  visibility: visible;
  opacity: 1;
}
.cd-top.cd-fade-out {
  opacity: .5;
}

.cd-top {
  display: inline-block;
  height: 40px;
  width: 40px;
  position: fixed;
  bottom: 100px;
  right: 10px;
  z-index: 10;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.05);
  overflow: hidden;
  text-indent: 100%;
  white-space: nowrap;
  background: &dtml-cor_principal; url(imagens/cd-top-arrow.svg) no-repeat center 50%;
  visibility: hidden;
  opacity: 0;
  -webkit-transition: opacity .3s 0s, visibility 0s .3s;
  -moz-transition: opacity .3s 0s, visibility 0s .3s;
  transition: opacity .3s 0s, visibility 0s .3s;
}
.cd-top.cd-is-visible, .cd-top.cd-fade-out, .no-touch .cd-top:hover {
  -webkit-transition: opacity .3s 0s, visibility 0s 0s;
  -moz-transition: opacity .3s 0s, visibility 0s 0s;
  transition: opacity .3s 0s, visibility 0s 0s;
}
.cd-top.cd-is-visible {
  visibility: visible;
  opacity: 1;
}
.cd-top.cd-fade-out {
  opacity: .5;
}
.no-touch .cd-top:hover {
  background-color: #e86256;
  opacity: 1;
}
@media only screen and (min-width: 768px) {
  .cd-top {
    right: 20px;
    bottom: 20px;
  }
}
@media only screen and (min-width: 1024px) {
  .cd-top {
    height: 60px;
    width: 60px;
    right: 30px;
    bottom: 30px;
  }
}

