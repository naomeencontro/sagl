    -moz-border-radius: 2px;
    border-radius: 2px;
    background-color: #4a4a4e;
    -webkit-box-shadow:  0px -1px 1px #cdcdcd, 0px 1px 1px #dcdcdc;
    -moz-box-shadow:  0px -1px 1px #cdcdcd, 0px 1px 1px #dcdcdc;
    box-shadow:  0px -1px 1px #cdcdcd, 0px 1px 1px #dcdcdc;
